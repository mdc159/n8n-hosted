# Minimal .env for local testing
# Copy to .env: cp .env.local.example .env
#
# WARNING: These are TEST values only - do NOT use in production!

# Required secrets (test values - NOT for production)
N8N_ENCRYPTION_KEY=test-only-not-for-production-key-32ch
N8N_USER_MANAGEMENT_JWT_SECRET=test-only-jwt-secret-key-32chars

# Domain / HTTPS (overridden by docker-compose.override.yml)
N8N_HOST=localhost
WEBHOOK_URL=http://localhost:8080/
CADDY_EMAIL=test@localhost
TZ=UTC

# Caddy basic auth (not used in local testing with Caddyfile.local)
BASIC_AUTH_USER=admin
BASIC_AUTH_HASH=$$2a$$14$$placeholder-not-used-in-local-testing

# MCP authentication (required for HTTP mode)
MCP_AUTH_TOKEN=local-test-token-not-for-production

# MCP management tools (optional - leave empty for read-only mode)
# To enable: generate API key from n8n UI > Settings > n8n API
N8N_API_URL=http://n8n:5678
N8N_API_KEY=
